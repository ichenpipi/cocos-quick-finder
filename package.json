  "version": "1.3.2.20210821",
  "description": "快速搜索定位项目中的文件，还可以快速打开场景和预制体...",
  "author": "陈皮皮 (ifaswind)",
  "author-info": {
    "email": "952157129@qq.com",
    "url": "https://chenpipi.cn",
    "wechat": "im_chenpipi",
    "git-home": "https://gitee.com/ifaswind",
    "official-account": "公众号「菜鸟小栈」"
  },
  "repository": "https://gitee.com/ifaswind/ccc-quick-finder",
  "license": "MIT",
  "main": "src/main/index.js",
  "contributions": {
    "menu": [
      {
        "path": "i18n:menu.extension/i18n:ccc-quick-finder.name",
        "label": "i18n:ccc-quick-finder.search",
        "icon": "/images/search.png",
        "message": "open-search-panel"
      },
      {
        "path": "i18n:menu.extension/i18n:ccc-quick-finder.name",
        "label": "i18n:ccc-quick-finder.settings",
        "icon": "/images/settings.png",
        "message": "open-settings-panel"
      },
      {
        "path": "i18n:menu.extension/i18n:ccc-quick-finder.name",
        "label": "i18n:ccc-quick-finder.checkUpdate",
        "icon": "/images/update.png",
        "message": "menu-check-update"
      }
    ],
    "messages": {
      "open-search-panel": {
        "public": true,
        "description": "打开文件搜索栏",
        "methods": [
          "openSearchPanel"
        ]
      },
      "open-settings-panel": {
        "public": true,
        "description": "打开设置面板",
        "methods": [
          "openSettingsPanel"
        ]
      },
      "menu-check-update": {
        "public": true,
        "description": "检查扩展是否有更新",
        "methods": [
          "menuCheckUpdate"
        ]
      }
    },
    "shortcuts": [
      {
        "message": "open-search-panel",
        "win": "F1",
        "mac": "F1"
      }
    ]
  }
}